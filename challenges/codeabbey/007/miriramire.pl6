#!/usr/bin/env perl6
use v6;
my @array = (289,	266,	505,	315,	267,	274,	93,	368,	158,	467,	548,	193,	96,	299,	324,	440,	524,	398,	475,	580,	526,	65,	69,	74,	296,	313,	287,	144,	433,	255,	389,	122,	490,	294);
loop (my $i = 0; $i < 34; $i++) {
    my $celcius = (@array[$i] - 32) *(5 / 9);
    my $a = sprintf("%d", $celcius);
    if $celcius - $a >= 0.5 {
        say $a + 1;
    } else {
        say $a;
    }
}
