#!/usr/bin/env perl6
use v6;
my @array1 = (0.45639646519,	0.810529754031,	0.979789468925,	0.115766591392,	0.386313075665,	0.331269888207,	0.963435140904,	0.787837376352,	0.843216825277,	0.561160150915,	0.60391865531,	0.646846191958,	0.834166103508,	0.0241635967977,	0.741567580495,	0.26836152235,	0.343892662786,	0.541267586406,	0.657002609689,	0.0849416288547,	0.341927391477,	0.0877733035013,	0.552554085851,	0.733454738278,	0.924711512402);
loop (my $i = 0; $i < 25; $i++) {
    my $result = @array1[$i]*10;
    given $result {
        when $result <= 1.666666 {
            say 1;
        }
        when 1.66666 < $result <= 3.3333333 {
            say 2;
        }
        when 3.3333333 < $result <= 5 {
            say 3;
        }
        when 5 < $result <= 6.6666666 {
            say 4;
        }
        when 6.6666666 < $result <= 8.33333333 {
            say 5;
        }
        default {
            say 6;
        }
    }
}
